{
  "name": "express-sql-api-boilerplate",
  "version": "1.0.0",
  "description": "Express + SQL API boilerplate",
  "type": "module",
  "main": "app/index.js",
  "scripts": {
    "start": "node app",
    "dev": "nodemon | pino-pretty",
    "pkg:install-postgres": "npm install --save pg pg-hstore",
    "pkg:remove-postgres": "npm uinstall pg pg-hstore",
    "pkg:install-sqlite": "npm install --save sqlite3",
    "pkg:remove-sqlite": "npm uinstall sqlite3",
    "db:migrate:up": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:create": "sequelize migration:create --name "
  },
  "keywords": [],
  "author": "aktsbot",
  "license": "ISC",
  "dependencies": {
    "argon2": "^0.31.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.7",
    "pino": "^8.16.2",
    "pino-http": "^8.5.1",
    "sequelize": "^6.37.5",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "pino-pretty": "^10.2.3",
    "prettier": "^3.1.0",
    "sequelize-cli": "^6.6.2"
  }
}
