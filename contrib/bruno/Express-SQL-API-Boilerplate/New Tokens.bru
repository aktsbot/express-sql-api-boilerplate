meta {
  name: New Tokens
  type: http
  seq: 4
}

post {
  url: {{apiUrl}}/api/auth/token
  body: json
  auth: bearer
}

headers {
  : 
}

auth:bearer {
  token: {{accessToken}}
}

body:json {
  {
      "refreshToken": "{{refreshToken}}"
  }
}

script:post-response {
  if(res.body.accessToken) {
    bru.setEnvVar('accessToken', res.body.accessToken);
    bru.setEnvVar('refreshToken', res.body.refreshToken);
  }
  
}
